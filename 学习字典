""")
seq=("Name","Age","Grade","Class")
Student1=dict.fromkeys(seq)
print(Student1)
del Student1
Student1=dict.fromkeys(seq,114)
Student1["Name"]="田所浩二"
Student1["Age"]=24
print("指定详细值",Student1)
print("-----------------------------------")
time.sleep(1)
print("Name:",Student1.get("Name"))
print("Age:",Student1.get("Age"))
print("Grade:",Student1.get("Grade"))
print("Class:",Student1.get("Class"))
print("""





""")
time.sleep(1)
print("是否存在“world”?","wolrd" in Student1.keys())
print("是否存在“Name”?","Name" in Student1.keys())
A="world" in Student1.keys()
if A=="True":
    print("“world” is in Student1")
else:
    print("“world” isn't in Student1")
